{
    // Editor settings
    "files.autoSave": "afterDelay",
    "window.commandCenter": true,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    // File associations and formatting
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": false,
    "files.trimFinalNewlines": true,
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "workbench.colorTheme": "IntelliJ IDEA New UI Dark",
    // "workbench.iconTheme": "vscode-jetbrains-icon-theme-2023-light",
    // "workbench.colorTheme": "IntelliJ IDEA New UI Light with Light Header",
    "workbench.iconTheme": "vscode-jetbrains-icon-theme",
    "workbench.productIconTheme": "jetbrains-idea-product-icon-theme",
    "openapi.platformUrl": "https://platform.42crunch.com",
    "openapi.platformAuthType": "anond-token",
    "vim.overrideCopy": false,
    "go.buildTags": "integration",
    "editor.lineNumbers": "relative",
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit",
        "source.fixAll": "explicit"
    },
    "editor.rulers": [
        120
    ],
    "editor.wordWrap": "off",
    "editor.wordWrapColumn": 120,
    // Go specific settings
    "go.useLanguageServer": true,
    "go.formatTool": "goimports",
    "go.lintTool": "golint",
    "go.vetOnSave": "package",
    "go.toolsManagement.autoUpdate": true,
    // SonarLint settings
    "sonarlint.rules": {
        "go:S103": {
            "level": "on"
        },
        "java:S103": {
            "level": "on",
            "parameters": {
                "maximumLineLength": "120"
            }
        },
        "javascript:S103": {
            "level": "on",
            "parameters": {
                "maximumLineLength": "120"
            }
        },
        "python:S103": {
            "level": "on",
            "parameters": {
                "maximumLineLength": "120"
            }
        },
        "typescript:S103": {
            "level": "on",
            "parameters": {
                "maximumLineLength": "120"
            }
        },
        "go:S107": {
            "level": "on",
            "parameters": {
                "maximumFunctionParameters": "5"
            }
        },
        "java:S107": {
            "level": "on",
            "parameters": {
                "maximumFunctionParameters": "5"
            }
        },
        "javascript:S107": {
            "level": "on",
            "parameters": {
                "maximumFunctionParameters": "5"
            }
        },
        "python:S107": {
            "level": "on",
            "parameters": {
                "maximumFunctionParameters": "5"
            }
        },
        "typescript:S107": {
            "level": "on",
            "parameters": {
                "maximumFunctionParameters": "5"
            }
        }
    },
    "sonarlint.connectedMode.project": {
        "connectionId": "",
        "projectKey": ""
    },
    // Language specific settings
    "[go]": {
        "editor.defaultFormatter": "golang.go",
        "editor.tabSize": 4,
        "editor.insertSpaces": false,
        "editor.detectIndentation": false,
        "editor.wordWrapColumn": 120,
        "editor.wordWrap": "off",
        "editor.rulers": [
            120
        ],
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        }
    },
    "[java]": {
        "editor.defaultFormatter": "redhat.java",
        "editor.tabSize": 4,
        "editor.insertSpaces": true,
        "editor.detectIndentation": false,
        "editor.rulers": [
            120
        ],
        "editor.wordWrapColumn": 120,
        "editor.wordWrap": "off"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
        "editor.detectIndentation": false,
        "editor.rulers": [
            120
        ],
        "editor.wordWrapColumn": 120,
        "editor.wordWrap": "off"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
        "editor.detectIndentation": false,
        "editor.rulers": [
            120
        ],
        "editor.wordWrapColumn": 120,
        "editor.wordWrap": "off"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.tabSize": 4,
        "editor.insertSpaces": true,
        "editor.detectIndentation": false,
        "editor.rulers": [
            120
        ],
        "editor.wordWrapColumn": 120,
        "editor.wordWrap": "off"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one",
        "editor.wordWrap": "on"
    },
    "jake.autoDetect": "on",
    "launch": {
        "configurations": [],
        "compounds": []
    },
    "makefile.configureOnOpen": true,
    "docker.extension.enableComposeLanguageServer": false,
    "git.openRepositoryInParentFolders": "never",
    "openapi.securityAuditToken": "",
    "openapi.platformServices": "",
    "openapi.platformScandManager": {
        "url": "",
        "auth": "none",
        "timeout": 300,
        "header": {
            "name": "",
            "value": ""
        }
    },
    "openapi.docker": {
        "replaceLocalhost": true,
        "useHostNetwork": true
    },
    "openapi.platformAuditRuntime": "platform",
    "openapi.platformConformanceScanRuntime": "cli",
    "openapi.platformConformanceScanImage": "42crunch/scand-agent:v2",
    "openapi.platformRepository": "https://repo.42crunch.com/downloads",
    "openapi.cliDirectoryOverride": "",
    "openapi.platformTemporaryCollectionName": "IDE Temp Collection",
    "openapi.platformMandatoryTags": "",
    "openapi.internalFeatures": false,
    "openapi.internalUseDevEndpoints": false,
    "openapi.approvedHostnames": []
}